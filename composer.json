{
    "description": "Abstract service class for retrieving data from Apis and caching the results with the Flow Framework",
    "type": "neos-package",
    "name": "shel/api-connector",
    "license": "MIT",
    "keywords": [
        "flow",
        "neoscms",
        "api",
        "caching"
    ],
    "require": {
        "neos/flow": "^7.3  || ^8.3",
        "php": ">=8.1",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Shel\\ApiConnector\\": "Classes"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Shel.ApiConnector"
        }
    }
}
